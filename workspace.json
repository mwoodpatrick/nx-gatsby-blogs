{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "defaultProject": "blog-wordpress",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "none"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "blog-medium": {
      "root": "apps/blog-medium",
      "sourceRoot": "apps/blog-medium/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/blog-medium/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "blog-medium:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "blog-medium:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/blog-medium/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/blog-medium"],
          "options": {
            "jestConfig": "apps/blog-medium/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "executor": "@netlify-builder/deploy:deploy",
          "options": {
            "outputPath": "apps/blog-medium/public",
            "siteId": "9e1790a3-42c0-4f40-9c4c-40396ae5728c"
          }
        }
      }
    },
    "blog-medium-e2e": {
      "root": "apps/blog-medium-e2e",
      "sourceRoot": "apps/blog-medium-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/blog-medium-e2e/cypress.json",
            "tsConfig": "apps/blog-medium-e2e/tsconfig.e2e.json",
            "devServerTarget": "blog-medium:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "blog-medium:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/blog-medium-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "blog-wordpress": {
      "root": "apps/blog-wordpress",
      "sourceRoot": "apps/blog-wordpress/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/blog-wordpress/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "blog-wordpress:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "blog-wordpress:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/blog-wordpress/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/blog-wordpress"],
          "options": {
            "jestConfig": "apps/blog-wordpress/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "executor": "@netlify-builder/deploy:deploy",
          "options": {
            "outputPath": "apps/blog-wordpress/public",
            "siteId": "54efdcd6-121a-46b9-aac1-b1dc6fb6b8fb"
          }
        }
      }
    },
    "blog-wordpress-e2e": {
      "root": "apps/blog-wordpress-e2e",
      "sourceRoot": "apps/blog-wordpress-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/blog-wordpress-e2e/cypress.json",
            "tsConfig": "apps/blog-wordpress-e2e/tsconfig.e2e.json",
            "devServerTarget": "blog-wordpress:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "blog-wordpress:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/blog-wordpress-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "nextjs-wp-frontend": {
      "root": "apps/nextjs-wp-frontend",
      "sourceRoot": "apps/nextjs-wp-frontend",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/nextjs-wp-frontend",
            "outputPath": "dist/apps/nextjs-wp-frontend"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "nextjs-wp-frontend:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "nextjs-wp-frontend:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "nextjs-wp-frontend:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nextjs-wp-frontend"],
          "options": {
            "jestConfig": "apps/nextjs-wp-frontend/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nextjs-wp-frontend/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "nextjs-wp-frontend-e2e": {
      "root": "apps/nextjs-wp-frontend-e2e",
      "sourceRoot": "apps/nextjs-wp-frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nextjs-wp-frontend-e2e/cypress.json",
            "tsConfig": "apps/nextjs-wp-frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "nextjs-wp-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nextjs-wp-frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nextjs-wp-frontend-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
